{
  "compilerOptions": {
    "module": "esnext",
    "importHelpers": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/vuetify/types"
    ],
  },
}
